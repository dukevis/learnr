#Import Shapefile, view, and merge

library(rgdal)
#Import shapefiles using readOGR from rgdal package
Durham1 = readOGR(dsn = "D:/LearnR/BasicMapping/DurhamBL1.shp", layer = "DurhamBL1")
Durham2 = readOGR(dsn = "D:/LearnR/BasicMapping/DurhamBL2.shp", layer = "DurhamBL2")

#View Durham1 in "Viewer"
plot(Durham1)

library(raster)
#Merge Durham1 and Durham2 shapefiles together using Raster library
MergeDurham<-union(Durham2, Durham1)
plot(MergeDurham)
